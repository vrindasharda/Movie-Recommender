<%- include("partials/header"); -%>
<%- include("partials/navbar"); -%>

<div class="row">

  <div class="row">
    <% for (var i = 0; i < data.length; i++) { %>
      <div class="col">
        <div class="card">
          <iframe src="<%= data[i].yUrl%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <div class="card-body">
            <h3 class="card-title"><%= data[i].Name %></h3>
            <p class="card-text">(<%= data[i].Type %>)</p>
            <p class="card-text"><%= data[i].wTeaser %></p>

          </div>
        </div>
      </div>
    <%} %>


  </div>



<%- include("partials/footer"); -%>
